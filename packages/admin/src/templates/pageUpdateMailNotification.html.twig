
<ul>
{% for page in pages %}
    <li>
        <code>{{ page.slug }}</code><br>
        <a href="{{ url('pushword_page', {slug: page.slug}) }}">{{ pw(page).h1 }}</a><br>
        {{ 'adminPageUpdateNotificationUpdatedOn'|trans }} {{ page.updatedAt|format_datetime('medium', 'short') }} <br>
        <a href="{{ url('admin_page_edit', {id: page.id}) }}">{{ 'adminPageUpdateNotificationEdit'|trans }}</a>
    </li>
{% endfor %}
</ul>
