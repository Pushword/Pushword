{
  "name": "pushword/admin",
  "type": "symfony-bundle",
  "description": "Default Admin to manage online Pushword App and websites.",
  "keywords": [],
  "homepage": "https://pushword.piedweb.com/extension/admin",
  "license": "MIT",
  "require": {
    "pushword/core": "*",
    "easycorp/easyadmin-bundle": "^4",
    "symfony/ux-twig-component": "^2",
    "twig/extra-bundle": "^3"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Pushword\\Admin\\": "src"
    }
  },
  "authors": [
    {
      "name": "Robin@PiedWeb",
      "email": "contact@piedweb.com",
      "homepage": "https://piedweb.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
